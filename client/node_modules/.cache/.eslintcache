te(false)\r\n    const history = useHistory()\r\n    const routeHomePage = () => {\r\n        history.push(\"/\")\r\n    }\r\n    const userType = localStorage.getItem(\"userType\")\r\n    const loggedIn = localStorage.getItem('isLoggedIn') ;\r\n\r\n    console.log('Here is the is logged in : ' ,loggedIn) ;\r\n    \r\n    const clearLocalStorage = () => {\r\n        localStorage.clear();\r\n        if (userType === \"user\") {\r\n            history.push(\"/\")\r\n        }\r\n    }\r\n\r\n    useEffect(() => {\r\n        console.log(\"state changed\") ;\r\n        setIsLoggedIn(loggedIn) ;\r\n    }, [localStorage.getItem('isLoggedIn')]) ;\r\n    \r\n    return (\r\n        <div>\r\n            <header className=\"headerOfBook\">\r\n                <div className=\"logoImage\">\r\n                    <img src={logoImg} onClick={() => { routeHomePage() }} alt=\"headerLogo\" /><Link to=\"/\"></Link>\r\n                </div>\r\n                <nav className=\"nav-barOf-Header\">\r\n                    <NavLink to=\"/NewArrival\">New Arrival</NavLink>\r\n                    <NavLink to=\"/bestsellers\">Bestsellers</NavLink>\r\n                    <NavLink to=\"/TextBooks\">Text book</NavLink>\r\n                    <NavLink to=\"/Featured Authors\">Featured Authors</NavLink>\r\n\r\n                </nav>\r\n                <div className=\"search-bar-header\">\r\n                    <i class=\"fas fa-search\" ></i> <input type=\"search\" placeholder=\"Search\" />\r\n                </div>\r\n                <div className=\"header-actions\">\r\n                    {\r\n                        (isLoggedIn && userType != 'admin') && <div className=\"actions\" >\r\n                        <i class=\"far fa-user\"></i>\r\n                        <NavLink to=\"/profile\">Profile</NavLink>\r\n                    </div>\r\n                    }\r\n                    {\r\n                        (isLoggedIn && userType != 'admin') && <div className=\"actions\" >\r\n                        <i class=\"far fa-heart\"></i>\r\n                        <NavLink to=\"/myWishlist\">Wishlist</NavLink>\r\n                    </div>\r\n                    }\r\n                    {console.log('consoling isLoggedInt : ', isLoggedIn)}\r\n                    {isLoggedIn == 'true' ? (<div className=\"actions\" >\r\n                        <i class=\"fas fa-sign-in-alt\"></i>\r\n                        <NavLink to=\"/Login\" onClick={() => { clearLocalStorage(); setIsLoggedIn(prevValue => !prevValue) }}>LogOut</NavLink>\r\n                    </div>) : <div className=\"actions\" >\r\n                        <i class=\"fas fa-sign-in-alt\"></i>\r\n                        <NavLink to=\"/Login\">LogIn</NavLink>\r\n                    </div>\r\n\r\n                    }\r\n                </div>\r\n            </header>\r\n        </div>\r\n    )\r\n}\r\nexport default Header","C:\\Users\\Amisha\\Desktop\\project\\client\\src\\components\\user-navbar\\navbar.jsx",[],"C:\\Users\\Amisha\\Desktop\\project\\client\\src\\components\\user_login-signup\\loginPage.jsx",[],"C:\\Users\\Amisha\\Desktop\\project\\client\\src\\components\\user_login-signup\\signUp.jsx",[],"C:\\Users\\Amisha\\Desktop\\project\\client\\src\\components\\homepage\\homePage.jsx",[],"C:\\Users\\Amisha\\Desktop\\project\\client\\src\\components\\textBooks\\textBook.jsx",["121","122"],"import React, { useState, useEffect } from \"react\";\r\nimport {useHistory} from 'react-router-dom' ;\r\nimport \"../common-products-css/commonProducts.css\"\r\nimport axios from \"axios\"\r\nimport buyNow from \"../commonFunction/buyNow\";\r\nimport wishList from \"../commonFunction/wishlist\";\r\n\r\nconst TextBook = () => {\r\n    const [textBooks, setTextBooks] = useState([]);\r\n    const history = useHistory() ;\r\n    const isLoggedIn = localStorage.getItem('isLoggedIn') ;\r\n    const getTextBooks = async () => {\r\n        const response = await axios.get(\"http://Localhost:8001/api/product/getAllProducts\");\r\n        console.log(\"response\", response);\r\n        const arrayOfData = []\r\n        for (let book of response.data) {\r\n            if (book.bookType === \"textBooks\") {\r\n                arrayOfData.push(book)\r\n            }\r\n        }\r\n        setTextBooks(arrayOfData);\r\n\r\n    }\r\n    useEffect(() => {\r\n        getTextBooks()\r\n    }, [])\r\n    return (\r\n        <div>\r\n            <h2 className=\"product-heading\">Text-books</h2>\r\n            <div className=\"parent-div\">\r\n                {textBooks.map((data, index) => {\r\n                    return (\r\n                        <div className=\"container-of-page\">\r\n                            {/* // <h2>New Arrival {index + 1}</h2> */}\r\n\r\n\r\n                            <div className=\"product-card\">\r\n                                <div className=\"product-image\">\r\n                                    <img src={data.image} alt=\"product image\"></img>\r\n                                </div>\r\n                                <div className=\"product-details\">\r\n                                    <h3>{data.bookName}</h3>\r\n                                    <h4>{data.author}</h4>\r\n                                    <div className=\"products-price\">\r\n                                        <span className=\"product-discountPrice\">Rs.{data.price}</span>\r\n                                        <span className=\"product-price\">{data.cutPrice}</span>\r\n                                    </div>\r\n                                </div>\r\n                                <div className=\"product-btns\">\r\n                                    <button className=\"product-wishlist\" onClick={() => wishList(data._id)}> <i class=\"far fa-heart\"> </i></button>\r\n\r\n                                    <button className=\"product-buy\" onClick={() => {\r\n                                        if(!isLoggedIn || isLoggedIn == 'false'){\r\n                                            return history.push('/login') ;\r\n                                        }\r\n                                        buyNow(data._id)\r\n                                    }}><i class=\"fas fa-shopping-bag\"></i>           Buy Now</button>\r\n                                </div>\r\n\r\n                            </div>\r\n\r\n                        </div>\r\n                    )\r\n                })}\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\nexport default TextBook;\r\n","C:\\Users\\Amisha\\Desktop\\project\\client\\src\\components\\newArrivals\\newArrival.jsx",["123","124"],"import React, { useEffect, useState } from \"react\";\r\nimport \"../common-products-css/commonProducts.css\"\r\nimport {useHistory} from 'react-router-dom' ;\r\nimport axios from \"axios\";\r\nimport buyNow from \"../commonFunction/buyNow\";\r\nimport wishList from \"../commonFunction/wishlist\";\r\n\r\nconst Newarrival = () => {\r\n\r\n    const history = useHistory() ;\r\n\r\n    const [newAArrivals, setNewArrivals] = useState([]);\r\n    const isLoggedIn = localStorage.getItem(\"isLoggedIn\") ;\r\n    const getNewArrivals = async () => {\r\n        const response = await axios.get(\"http://Localhost:8001/api/product/getAllProducts\");\r\n        console.log(\"response\", response);\r\n        const arrayOfData = []\r\n        for (let book of response.data) {\r\n            if (book.bookType === \"newArrivals\") {\r\n                arrayOfData.push(book)\r\n            }\r\n        }\r\n        setNewArrivals(arrayOfData);\r\n\r\n    }\r\n    useEffect(() => {\r\n        getNewArrivals()\r\n    }, [])\r\n\r\n\r\n    return (\r\n        <div>\r\n            <h2 className=\"product-heading\">New arrivals</h2>\r\n            <div className=\"parent-div\">\r\n            {newAArrivals.map((data, index) => {\r\n                return (\r\n                    <div className=\"container-of-page\">\r\n                        {/* // <h2>New Arrival {index + 1}</h2> */}\r\n\r\n\r\n                        <div className=\"product-card\">\r\n                            <div className=\"product-image\">\r\n                                <img src={data.image} alt=\"product image\"></img>\r\n                            </div>\r\n                            <div className=\"product-details\">\r\n                                <h3>{data.bookName}</h3>\r\n                                <h4>{data.author}</h4>\r\n                                <div className=\"products-price\">\r\n                                    <span className=\"product-discountPrice\">Rs.{data.price}</span>\r\n                                    <span className=\"product-price\">{data.cutPrice}</span>\r\n                                </div>\r\n                            </div>\r\n                            <div className=\"product-btns\">\r\n                                <button className=\"product-wishlist\" onClick={() => wishList(data._id)}> <i class=\"far fa-heart\"> </i></button>\r\n\r\n                                <button className=\"product-buy\" onClick={() => {\r\n                                    if(!isLoggedIn || isLoggedIn == 'false'){\r\n                                        return history.push('/login') ;\r\n                                    }\r\n                                    buyNow(data._id)\r\n                                    }}><i class=\"fas fa-shopping-bag\"></i>Buy Now</button>\r\n                            </div>\r\n\r\n                        </div>\r\n\r\n                    </div>\r\n                )\r\n            })}\r\n        </div>\r\n        </div>\r\n    )\r\n}\r\nexport default Newarrival;\r\n","C:\\Users\\Amisha\\Desktop\\project\\client\\src\\components\\customer-profile\\profile.jsx",["125","126","127","128","129","130","131"],"import React, { useState, useEffect } from \"react\"\r\nimport axios from \"axios\"\r\nimport \"../loader/loader.css\"\r\nimport \"../customer-profil